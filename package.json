    "jest": {
        "verbose": true,
        "setupFilesAfterEnv": [
            "<rootDir>/src/frontend/__test__/setupTest.js"
        ],
        "moduleNameMapper": {
            "\\.(jp|jpeg|png|gif)$": "<rootDir>/src/frontend/__mocks__/fileMock.js",
            "\\.(scss|css|sass)$": "<rootDir>/src/frontend/__mocks__/styleMock.js"
