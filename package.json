        "start:dev": "nodemon src/server/index",
        "build": "webpack --config webpack.prod.js",
        "dev": "webpack serve --open --config webpack.dev.js",
        "lint": "eslint ./src/ --fix",
        "prepare": "husky install",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:ci": "jest --coverage"
    },
    "engines": {
